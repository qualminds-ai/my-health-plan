{
  "name": "my-health-plan-server",
  "version": "1.0.0",
  "description": "Backend server for MyHealthPlan demo",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --ext js,json,sql --ignore node_modules/ --ignore ../client/ server.js",
    "debug": "nodemon --inspect --ext js,json,sql --ignore node_modules/ --ignore ../client/ server.js",
    "db:setup": "node scripts/db-setup.js",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:status": "node scripts/migration-manager.js status",
    "db:create": "node scripts/migration-manager.js create",
    "db:reset": "npm run db:setup",
    "db:check": "node scripts/migration-check.js",
    "lint": "eslint . --ext .js --quiet",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0"
  }
}