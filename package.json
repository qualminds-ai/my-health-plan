{
  "name": "my-health-plan",
  "version": "1.0.0",
  "description": "MyHealthPlan - Healthcare Authorization Management System (Client-Only)",
  "scripts": {
    "start": "npm run client:dev",
    "dev": "npm run client:dev",
    "dev:debug": "npm run client:dev",
    "install:all": "npm install && cd client && npm install",
    "client:dev": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "client:build:watch": "cd client && npm run build:watch",
    "build": "npm run client:build",
    "test": "cd client && npm test",
    "watch": "npm run client:build:watch",
    "lint": "cd client && npm run lint",
    "clean": "rimraf client/build node_modules client/node_modules"
  },
  "dependencies": {},
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}